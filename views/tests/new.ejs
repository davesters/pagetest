<% layout(layoutPath) -%>

<form action="/tests/new" method="post">
  <fieldset>
    <label>Name:</label>
    <input type="text" name="name" value="<%= test.name || "" %>"/>
    <label>URL:</label>
    <input type="text" name="url" value="<%= test.url || "" %>"/><br/>
    <button class="btn">Create Test</button>
  </fieldset>
</form>